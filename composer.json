{
    "name": "reactphp-x/limiter",
    "description": "A generic rate limiter for the web and reactphp. Useful for API clients, web crawling, or other tasks that need to be throttled",
    "keywords": ["limiter", "reactphp", "rate-limiter", "bucket"],
    "require": {
        "php": "^8.1",
        "react/promise": "^3",
        "react/async": "^4.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ReactphpX\\Limiter\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ReactphpX\\Limiter\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "authors": [
        {
            "name": "wpjscc"
        }
    ]
}
